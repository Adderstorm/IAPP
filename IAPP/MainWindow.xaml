<Window x:Class="IAPP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IAPP"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="450" MinWidth="800"
        Width="800" Height="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="35"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#FF329ED8"/>
        <Grid Grid.Row="2" Background="#FF329ED8"/>
        <Frame Name="MEF" Grid.Row="1" ContentRendered="MEF_ContentRendered" NavigationUIVisibility="Hidden"/>
        <Button x:Name="BtnBack" Click="BtnBack_Click" Height="30" Width="100" Grid.Row="0" HorizontalAlignment="Right" Margin="0,0,10,0" Content="Back" FontSize="15"/>
    </Grid>
</Window>
